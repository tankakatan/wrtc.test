{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~*": ["./src/*"],
            "Common": ["./src/Common"],
            "Common/*": ["./src/Common/*"]
        },
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "allowJs": false,
        "noImplicitAny": true
    },
    "include": ["src/**/*"]
}